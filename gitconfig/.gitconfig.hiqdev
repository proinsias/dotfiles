[commit]
    template = ~/.stCommitMsg
[core]
    excludesfile = ~/.gitignore
[credential]
    helper = cache --timeout 3600
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[help]
    format = man
[include]
    path = ~/.gitconfig.include
[merge]
    # tool = emerge
    tool = p4merge
