# conda

alias activate_conda='source is2dev_conda.sh python3'
alias activate_conda2='source is2dev_conda.sh python2'
alias activate_sys='source is2dev_conda.sh sys'
alias deactivate_conda="source is2dev_conda.sh deactivate"

# Repos
alias is2='cd /usr/local/insightsquared'
alias conf='cd /usr/local/insightsquared/configuration'
alias screens='cd /usr/local/insightsquared/configuration/screens2'
alias ise='cd /usr/local/insightsquared/is_engine'
alias isservice='cd /usr/local/insightsquared/is_service/'
alias ist='cd /usr/local/insightsquared/is_tools/'
alias isk='cd /usr/local/insightsquared/is_engine/bbase/utils/is_charter2/'
alias projects='cd /usr/local/insightsquared/projects/'
alias fv6='cd /usr/local/insightsquared/forecastv6/'

alias dis2='cd ~/devbox/usr/local/insightsquared'
alias dconf='cd ~/devbox/usr/local/insightsquared/configuration'
alias dscreens='cd ~/devbox/usr/local/insightsquared/configuration/screens2'
alias dise='cd ~/devbox/usr/local/insightsquared/is_engine'
alias dice='cd ~/devbox/usr/local/insightsquared/is_ice/'
alias dist='cd ~/devbox/usr/local/insightsquared/is_tools/'
alias disk='cd ~/devbox/usr/local/insightsquared/is_engine/bbase/utils/is_charter2/'
alias dprojects='cd ~/devbox/usr/local/insightsquared/projects/'
alias dfv6='cd ~/devbox/usr/local/insightsquared/forecastv6/'

#alias tw='touch /usr/local/insightsquared/is_engine/bbase/wsgi_handler.py'
alias tw='sudo /etc/init.d/gunicorn restart'

# Extra tools
alias migrate='python /usr/local/insightsquared/is_engine/bbase/manage.py migrate'
alias dbshell='python -i /usr/local/insightsquared/is_tools/user_scripts/foxtrot/model_db.py'
alias dbshellb='bpython -i /usr/local/insightsquared/is_tools/user_scripts/foxtrot/model_db.py'

# Syncs credentials for dev boxes
# Add --get-input to overwrite existing 'Input_' tables
# Add --source to get raw provider data tables
# Add --db-tmp to get 'insightsquared_<client>' tables
# Leave off --db-tmp to get 'insightsquared' tables
# Add --main to get 'main_' and 'isconfig_' tables
alias issink='python /usr/local/insightsquared/is_engine/issink/issink.py --get-input -v --db-tmp --source'

# Fuzzes an URL, attempting to give it valid, semi-valid, and invalid inputs to various URL parameters.
# usage: --urls=bbase/bin/smoke_test.txt
# Does the same smoke testing that engine pushes do.
# -m <report_path>
# Tests any report with a url that matches the regex <report_path>
alias test_site='python /usr/local/insightsquared/is_engine/bbase/bin/test_site.py -u -j -t 100'

# Allows you to run a host of things with is2 imports and settings.
alias run='python /usr/local/insightsquared/is_engine/bbase/bin/run.py'

# Entry point to megatest-based footing and regression testing.
# USAGE:
#   megatest -c <client> -u <url_path_query_params_optional> [finch|footing]
alias megatest='run -m bbase.megatest'

# Alias for shell_plus, the only is2 Python REPL you need.
alias shellplus='python /usr/local/insightsquared/is_engine/bbase/manage.py shell_plus'

# Watches in configuration|is_engine, and touches the watcher on change.
alias watch='python /usr/local/insightsquared/is_engine/bbase/bin/watcher.py'

# Tails the debug_stream.log file.
alias debugstream='tail -F /usr/local/insightsquared/logs/debug_stream.log'

# Executes a client-contextual request to an endpoint using the Django Test Client. get is in is_tools/andy
alias get='python -m megatest.get'

# Runs the is2 Django app locally in debug mode, allowing complex debug scenarios (AJAX requests sent from template-provided JS)
alias runserver='/usr/bin/python /usr/local/insightsquared/is_engine/bbase/manage.py runserver 0.0.0.0:8001 --nostatic'

alias foot='python /usr/local/insightsquared/is_engine/bbase/bin/test_foot.py -v -d'
alias test_config='python /usr/local/insightsquared/is_engine/isdata/bin/test_config.py'
alias gen_report='python /usr/local/insightsquared/configuration/screens2/gen_report.py'
